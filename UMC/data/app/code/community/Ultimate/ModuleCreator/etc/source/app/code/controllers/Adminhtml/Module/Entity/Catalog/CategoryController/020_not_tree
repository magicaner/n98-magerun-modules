
    /**
     * {{entities}} grid in the catalog page
     *
     * @access public
     * @return void
     * {{qwertyuiop}}
     */
    public function {{entities}}gridAction()
    {
        $this->_initCategory();
        $this->loadLayout();
        $this->getLayout()->getBlock('category.edit.tab.{{entity}}')
            ->setCategory{{Entities}}($this->getRequest()->getPost('category_{{entities}}', null));
        $this->renderLayout();
    }
